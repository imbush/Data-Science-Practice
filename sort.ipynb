    "def selection_sort(arr: list):\n",
    "    for i in range(len(arr)):\n",
    "        for j in range(i + 1, len(arr)): # loops through items after i\n",
    "            if arr[j] < arr[i]: #switches if lower than arr[i]\n",
    "                arr[i], arr[j] = arr[j], arr[i]\n",
    "    return arr"
   ]
